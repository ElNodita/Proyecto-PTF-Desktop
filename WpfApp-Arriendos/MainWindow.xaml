<Window x:Class="WpfApp_Arriendos.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp_Arriendos"
        mc:Ignorable="d"
        Title="Inicio de Sesión" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="txtCorreo" HorizontalAlignment="Left" Height="23" Margin="308,196,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="214"/>
        <PasswordBox x:Name="txtPass" HorizontalAlignment="Left" Margin="308,246,0,0" VerticalAlignment="Top" Width="214"/>
        <Button x:Name="btnIniciar" Content="Iniciar" HorizontalAlignment="Left" Margin="373,328,0,0" VerticalAlignment="Top" Width="75" Click="BtnIniciar_Click"/>
        <Label x:Name="lblmensaje" Content="" HorizontalAlignment="Left" Margin="532,275,0,0" VerticalAlignment="Top"/>
        <Image x:Name="image" HorizontalAlignment="Left" Height="114" Margin="335,52,0,0" VerticalAlignment="Top" Width="163" Source="Assets/img/logo.png"/>
    </Grid>
</Window>
